# main.py

# Import functions from preprocessing.py
from preprocessing import clean_data, encode_data

def main():
    # Example dataset
    data = {"some": "data"}

    # Call the preprocessing functions
    cleaned_data = clean_data(data)
    encoded_data = encode_data(cleaned_data)

    # Continue with other operations
    print("Final data:", encoded_data)

if __name__ == "__main__":
    main()