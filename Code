
# Check if the prediction is within 20% of the actual value
def check_within_20_percent(actual, predicted):
    result = []
    for act, pred in zip(actual, predicted):
        if abs(act - pred) / act <= 0.20:  # Check if the difference is within 20% of actual value
            result.append("Good Value")
        else:
            result.append("Bad Value")
    return result

# Apply the function
results = check_within_20_percent(y_test, y_pred)

# Output the result
output = pd.DataFrame({'Actual': y_test, 'Predicted': y_pred, 'Result': results})
print(output)