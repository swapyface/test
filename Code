import pandas as pd

# Sample DataFrame
data = {
    'date_column': ['2024-11-01', '2024-11-02', 'invalid_date', None, '2024-11-05']
}
df = pd.DataFrame(data)

# Initialize an empty list to store problematic records
invalid_records = []

# Check each value in the column
for index, value in df['date_column'].items():
    try:
        pd.to_datetime(value)  # Try converting to datetime
    except Exception as e:
        invalid_records.append((index, value, str(e)))  # Store index, value, and error message

# Display invalid records
for record in invalid_records:
    print(f"Index: {record[0]}, Value: {record[1]}, Error: {record[2]}")