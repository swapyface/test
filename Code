import pandas as pd
import numpy as np

# Example DataFrame
data = {'categories_column': ['A', 'B', None, 'C', '']}
df = pd.DataFrame(data)

# Replace empty strings or other placeholders with NaN
df['categories_column'].replace(['', 'unknown'], np.nan, inplace=True)

# Drop rows with NaN
df = df.dropna(subset=['categories_column'])

print(df)