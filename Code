import pandas as pd
import numpy as np

# Sample DataFrame
data = {'Category': ['A', 'B', 'C', 'A', 'D', 'B', 'E']}
df = pd.DataFrame(data)

# Categories to drop
categories_to_drop = ['B', 'D']

# Replace unwanted categories with NaN
df['Category'] = df['Category'].replace(categories_to_drop, np.nan)

# Drop rows with NaN if needed
df = df.dropna(subset=['Category'])

print(df)





df['date_column'] = pd.to_datetime(df['date_column']).astype('datetime64[ms]')
import pandas as pd
import numpy as np

# Example: replace out-of-range dates with NaT (Not a Time)
df['date_column'] = pd.to_datetime(df['date_column'], errors='coerce')