import pandas as pd

# Sample data
data = {'rating': ['AAA Stable', 'BBB- Negative', 'AA+ Positive', 'A Watch', 'BB+ Developing']}
df = pd.DataFrame(data)

# Clean the rating column: Keep only letters, numbers, '-', and remove anything after a space
df['rating_cleaned'] = df['rating'].str.replace(r'[^a-zA-Z0-9-]| .*', '', regex=True)

print(df)