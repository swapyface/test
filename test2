class Solution:
    def reverseWords(self, s: str) -> str:
        s = s.strip() # using strip to remove the extra space at the start and end of the string
        print(s)
        words = s.split() #using split to split the string's words into different words in a list
        print(words)
        reversed_words = words[::-1] # reversing the order using -1
        print(" ".join(reversed_words))
        return " ".join(reversed_words) # returning the joined string with single space