import pandas as pd

# Example dates
df = pd.DataFrame({
    'issue_date': pd.to_datetime(['2015-06-01', '2018-09-15']),
    'mature_date': pd.to_datetime(['2025-06-01', '2030-09-15'])
})

# Calculate number of years (as float)
df['maturity_years'] = (df['mature_date'] - df['issue_date']).dt.days / 365.25

# If you want full years only (integer)
df['maturity_years_int'] = (df['mature_date'] - df['issue_date']).dt.days // 365

print(df)